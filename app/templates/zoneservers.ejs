<div class="col-12">
    <div class="page-header">
        <h1 class="page-title">
            Zone Servers
        </h1>
    </div>

    <div class="card">
        <div class="table-responsive">
            <table class="table table-hover table-outline table-vcenter text-nowrap card-table">
                <thead>
                    <tr>
                        <th class="text-center w-1"><i class="icon-people"></i></th>
                        <th class="text-center w-1"><i class="icon-people"></i></th>
                        <th>Zone</th>
                        <th>Type</th>
                        <th>Short Name</th>
                        <th>Zone ID</th>
                        <th>Instance ID</th>
                        <th>Process ID</th>
                        <th>Port</th>
                        <th></th>
                    </tr>
                </thead>
                <tbody>
                <% zonelist.forEach(function(zone) { %>
                    <% /* if (zone.zone_long_name != "") { */ %>
                        <tr style="<%= zone.zone_long_name === '' ? 'color:rgba(0, 0, 0, 0.4);' : '' %>">
                            <td class="text-center">
                                <div class="avatar d-block" style="background-image: url(/assets/images/eqemu-avatar.png)">
                                    <span class="avatar-status bg-<%= zone.zone_long_name !== '' ? 'green' : 'red' %>"></span>
                                </div>
                            </td>

                            <td><i class="fe fe-users"></i> <%= zone.number_players %> </td>
                            <td><%= zone.zone_long_name %> </td>
                            <td>
                                <%= zone.is_static_zone === true ? 'Static' : 'Dynamic' %>
                            </td>
                            <td><%= zone.zone_name %> </td>
                            <td><%= zone.zone_id %> </td>
                            <td><%= zone.instance_id %> </td>
                            <td><%= zone.zone_os_pid %> </td>
                            <td><%= zone.client_port %> </td>
                            <td class="text-right">
                                <a href="/zoneserver/<%= zone.client_port %>/netstats" class="btn btn-secondary btn-sm">
                                    <i class="fa fa-sitemap" style="font-size:10px; vertical-align:0px"></i> Netstats
                                </a>
                            </td>
                        </tr>
                    <% /* } */ %>
                <% }); %>
                </tbody>
            </table>
        </div>
    </div>
</div>